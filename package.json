    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.2",
    "graphql": "^16.11.0",
    "jose": "^6.1.0",
    "next": "15.5.2",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rxjs": "^7.8.2",
